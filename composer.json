{
    "name": "wardish/phpexcel",
    "description": "PHP library for writing and reading files in EXCEL format. Modified to work on PHP 8. Forked from DisWEBru/phpexcel",
    "keywords": ["PHP","Excel","OpenXML","xlsx","xls","spreadsheet"],
    "homepage": "https://github.com/wardish/phpexcel",
    "type": "library",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Tetsuya Mito",
            "homepage": "https://www.wardish.jp"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*"
    },
    "recommend": {
        "ext-zip": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-0": {
            "PHPExcel": "Classes/"
        }
    }
}
